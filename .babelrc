{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": "> 0.25%, ie 11, not op_mini all, not dead"
        },
        "useBuiltIns": "usage",
        "corejs": "corejs@2"
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./src",
          "_root": "."
        }
      }
    ]
  ]
}
